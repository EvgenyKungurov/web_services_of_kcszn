<p id="notice"><%= notice %></p>

<h1>Employment Centers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employment_centers.each do |employment_center| %>
      <tr>
        <td><%= link_to 'Show', employment_center %></td>
        <td><%= link_to 'Edit', edit_employment_center_path(employment_center) %></td>
        <td><%= link_to 'Destroy', employment_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employment Center', new_employment_center_path %>
